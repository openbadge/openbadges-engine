{
  "name": "openbadges-engine",
  "version": "0.0.0",
  "description": "Open Badges' CMS based on Express framework",
  "keywords": [
    "openbadges",
    "engine",
    "CMS"
  ],
  "homepage": "https://github.com/openbadge/openbadges-engine",
  "bugs": "https://github.com/openbadge/openbadges-engine/issues",
  "author": {
    "name": "Evgeniy Gavryushin",
    "email": "job.egavr@yandex.ru",
    "url": "https://github.com/eGavr"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/openbadge/openbadges-engine.git"
  },
  "main": "lib/app.js",
  "bin": {
    "openbadges": "./bin/openbadges"
  },
  "dependencies": {
    "openbadges-issuer-api": "https://github.com/openbadge/openbadges-issuer-api/archive/master.tar.gz",
    "express": "4.9.4",
    "serve-static": "1.1.0",
    "body-parser": "1.8.3",
    "formidable": "1.0.15",
    "jws": "0.2.6",
    "request": "2.44.0",
    "is-png": "1.0.0",
    "passport": "0.2.1",
    "passport-local": "1.0.0",
    "express-session": "1.8.2",
    "optimist": "0.6.1",
    "cocaine": "0.4.1-4",
    "borschik": "1.3.0",
    "borschik-tech-cleancss": "1.0.0",
    "bower": "1.3.9",
    "bower-npm-install": "0.5.9",
    "enb": "0.13.7",
    "enb-bem": "0.1.0-beta2",
    "enb-autoprefixer": "0.1.1",
    "enb-bemxjst": "1.3.2",
    "enb-borschik": "1.1.1",
    "enb-diverse-js": "0.1.0",
    "enb-modules": "0.2.0",
    "enb-stylus": "1.1.2",
    "stylus": "0.47.1",
    "ym": "0.1.0",
    "uid": "0.0.2",
    "cookie-parser": "^1.3.3"
  },
  "devDependencies": {
    "jshint": "2.5.3",
    "jscs": "1.5.9"
  },
  "scripts": {
    "start": "bin/openbadges",
    "test": "npm run lint",
    "lint": "jshint . && jscs -c .jscs.js .",
    "postinstall": "./npm-postinstall",
    "deps": "bower-npm-install --non-interactive"
  }
}
